{
  "swagger": "2.0",
  "info": {
    "version": "1.0",
    "title": "Sqlite Fuzzer client"
  },
  "basePath": "/fuzz/plugin/sqlite",
  "tags": [
    {
      "name": "Sqlite Fuzzer ",
      "description": "\n Sqlite Fuzzer plugin "
    }
  ],
  "schemes": [
    "http"
  ],
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/status": {
      "get": {
        "tags": [
          "Get status"
        ],
        "description": "Gets status details",
        "summary": "Gets status details",
        "parameters": [],
        "responses": {
          "200": {
            "description": "Sqlite  Status details"
          }
        }
      }
    },
    "/do/{command}": {
      "get": {
        "tags": [
          "Fuzzer sqlite command "
        ],
        "summary": "Execute sqlite given command",
        "parameters": [
          {
            "name": "command",
            "in": "path",
            "description": "sqlite command",
            "required": true,
            "type": "string",
            "enum": [
              "run",
              "stop",
              "pause",
              "restart"
            ]
          }
        ],
        "description": "Execute fuzzer given command",
        "responses": {
          "200": {
            "description": "Returns <code>200</code> Fuzzer command execution indication or <code>404</code>"
          },
          "404": {
            "description": "Not Found"
          }
        }
      }
    }
  },
  "definitions": {
    "Command": {
      "type": "object",
      "properties": {
        "concurrency": {
          "type": "integer",
          "format": "int32"
        },
        "timeout": {
          "type": "integer",
          "format": "int32"
        },
        "role": {
          "type": "string"
        },
        "data": {
          "type": "string"
        }
      },
      "example": {
        "role": "sqlite",
        "concurrency": 5,
        "timeout": 18000000,
        "data": {
          "params": "-po=4"
        }
      }
    }
  }
}
